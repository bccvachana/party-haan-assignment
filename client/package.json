{
  "name": "party-haan-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n react,tsc -k \"yarn type-check && yarn lint && yarn start\" \"yarn tsc:watch\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "type-check": "tsc --pretty",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "tsc:watch": "yarn tsm src -w"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/parser": "^4.10.0",
    "concurrently": "^5.3.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "node-sass": "4.14.1",
    "typed-scss-modules": "^3.4.0",
    "typesafe-actions": "^5.1.0",
    "typescript": "^4.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}